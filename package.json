{
  "name": "graph-topics",
  "version": "0.1.0",
  "author": "Andrii Prykhodko",
  "license": "CC BY-NC 4.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "20.9.3",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.2.0",
    "axios": "^1.3.4",
    "css-mediaquery": "^0.1.2",
    "happy-dom": "^12.10.3",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-async": "^10.0.1",
    "react-dom": "^18.2.0",
    "react-force-graph": "^1.42.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.20.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "storybook-addon-react-router-v6": "^2.0.10",
    "typescript": "^4.9.5",
    "vitest": "^0.34.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:mock": "REACT_APP_MOCK=true react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "NODE_ENV=test npx vitest",
    "storybook": "storybook dev -p 6006",
    "test:component": "test-storybook",
    "build-storybook": "storybook build",
    "initialize-msw": "npx msw init ./public",
    "lint": "eslint src --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:storybook/recommended"
    ],
    "rules": {
      "quotes": [
        2,
        "single",
        {
          "avoidEscape": true
        }
      ],
      "playwright/no-eval": 0,
      "no-useless-catch": 1,
      "playwright/no-skipped-test": 0,
      "max-len": [
        1,
        {
          "code": 80
        }
      ],
      "indent": [
        "error",
        4
      ],
      "semi": [
        1,
        "never"
      ],
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": false,
          "ignoreComments": false
        }
      ],
      "no-prototype-builtins": 0,
      "arrow-parens": [
        2,
        "as-needed"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/preset-create-react-app": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/test": "^7.6.0-beta.2",
    "@storybook/test-runner": "^0.15.2",
    "@storybook/testing-library": "^0.2.2",
    "@types/css-mediaquery": "^0.1.4",
    "@types/ramda": "^0.29.9",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-plugin-storybook": "^0.6.15",
    "msw": "^1.3.2",
    "msw-storybook-addon": "^1.10.0",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.11.0",
    "storybook": "^7.5.3",
    "webpack": "^5.89.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
